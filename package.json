{
  "name": "uplot-react-js",
  "version": "0.0.2",
  "description": "\"This is chart wrapper for uplot library\"",
  "repository": {
    "type": "git",
    "url": "https://github.com/Duc-Developer/uplot-react"
},
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rm -rf dist/ && prettier --write src/ && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module CommonJS --outDir dist/cjs"
  },
  "engines": {
    "node": ">=8.10"
  },
  "keywords": [
    "Charts",
    "uPlot",
    "React",
    "Wrapper"
  ],
  "author": "Duc-Developer",
  "license": "MIT",
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@types/node": "^17.0.21",
    "@types/react": "^16.8.25",
    "@types/react-dom": "^16.8.5",
    "prettier": "^2.7.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "typescript": "^4.5.2",
    "uplot": "^1.6.7"
  }
}
